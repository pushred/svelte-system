{
  "name": "svelte-system",
  "type": "module",
  "private": true,
  "license": "UNLICENSED",
  "workspaces": [
    "packages/*",
    "web"
  ],
  "version": "0.0.1",
  "prettier": "@pushred/prettier-config",
  "scripts": {
    "lint": "eslint",
    "test": "jest",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "lint-staged": {
    "*.{json,md}": "prettier --log-level warn --write",
    "*.js": "prettier-eslint --log-level warn --write"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-modules-commonjs": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@pushred/eslint-config": "^1.0.0",
    "@pushred/prettier-config": "^1.0.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/svelte": "^3.0.3",
    "babel-jest": "^27.3.1",
    "del": "^6.0.0",
    "eslint": "^7.22.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^12.1.2",
    "pinst": "^2.1.6",
    "prettier": "~2.2.1",
    "prettier-eslint-cli": "^5.0.1",
    "svelte": "^3.44.1",
    "svelte-jester": "^2.1.5"
  }
}
